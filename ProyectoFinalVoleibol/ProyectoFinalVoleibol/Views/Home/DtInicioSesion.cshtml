@model ProyectoFinalVoleibol.Models.Directortecnico
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{ Layout = "_Layout"; }
<main>
    <section class="admin">
        <h1>Inicio sesión para directores técnicos</h1>
        <a href="~/Home/AdminIS">ACCEDER COMO ADMINISTRADOR</a>
        <form method="post" action="~/Home/DtInicioSesion">
            <input type="number" asp-for="Clave" placeholder="Clave" required />
            <br />
            <input type="password" asp-for="Contrasena" placeholder="contraseña" required />
            <br />
            <input class="boton" type="submit" value="Acceder como director tecnico" />
            @Html.ValidationSummary()
        </form>
    </section>
</main>